class Solution {
    public int reversePairs(int[] nums) {
        int l=nums.length;
        int a,count=0;
        for(int i=0;i<l-1;i++)
        {
            for(int j=i+1;j<l;j++)
            {
                //a=nums[j]*2;
                if(nums[i]>nums[j]*2L)
                count++;
            }
        }
      return count;  
    }
}
